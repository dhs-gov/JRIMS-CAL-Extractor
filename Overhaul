import re
import pandas as pd

def extract_numbers(excel_file, number_column, description_column):
    df = pd.read_excel(excel_file, engine='openpyxl')

    number_data = df[number_column]
    description_data = df[description_column]

    number_description_pairs = set()

    for idx, value in enumerate(number_data):
        matches = re.findall(r'\b\d{1,2}\.\d{1,2}\.\d{1,2}(?:\.\d{1,2})?\b', str(value))
        for match in matches:
            number_description_pairs.add((match, description_data[idx]))

    if not number_description_pairs:
        print("No such values.")
        return

    return number_description_pairs

number_description_pairs = extract_numbers('FILE.xlsx', 'Number_Column', 'Description_Column')

if number_description_pairs:
    for number, description in number_description_pairs:
        print(number, "-", description)

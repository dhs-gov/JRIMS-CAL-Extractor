import re
import PyPDF2
import pandas as pd

def extract_numbers_from_pdf(pdf_file):
    with open(pdf_file, 'rb') as file:
        pdf_reader = PyPDF2.PdfReader(file)

        text = ""

        for page in pdf_reader.pages:
            text += page.extract_text()

        numbers = re.findall(r'\b\d{1,2}\.\d{1,2}\.\d{1,2}(?:\.\d{1,2})?\b', text)
        unique_numbers = set(numbers)

        return unique_numbers

def extract_matching_values_from_excel(excel_file, number_column, description_column, pdf_numbers):
    df = pd.read_excel(excel_file, engine='openpyxl')

    for idx, excel_number in enumerate(df[number_column]):
        if excel_number in pdf_numbers:
            print(excel_number, "-", df[description_column][idx])

pdf_file = 'FILE.pdf'  # Replace with PDF file's name
excel_file = 'FILE.xlsx'  # Replace with Excel file's name
number_column = 'Number_Column'  # Replace with number column's name
description_column = 'Description_Column'  # Replace with description column's name

pdf_numbers = extract_numbers_from_pdf(pdf_file)
if not pdf_numbers:
    print("No such values.")
else:
    extract_matching_values_from_excel(excel_file, number_column, description_column, pdf_numbers)
